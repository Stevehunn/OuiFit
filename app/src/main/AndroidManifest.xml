<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ouifit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OuiFit">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".InscriptionActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".MenuCourseActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".MenuExercicesActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".MenuStatActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".ExerciceAbdoActivity"
            android:label="@string/app_name">
        </activity>


    </application>

</manifest>